<!DOCTYPE html>
<html>

	<%- include('partials/head') %>

	<body>

		<%- include('partials/navbar', {links:[ {text: "Profile", url:"/users/"+user.username}, {text: "Logout", url: "/users/logout"} ]}) %>


		<div class="container">

			<div class="row">

				<% videos.forEach(function(video){ %>
					<div class="card">
						<div class="card-action">
							<a href="/<%= video.UID %>"><%= video.name %></a>
						</div>
					</div>
				<% }); %>

			</div>

		</div>

	</body>

</html>